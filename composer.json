{
    "name": "lochac/seneschals-database",
    "type": "project",
    "require": {
        "google/apiclient": "1.1.4",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-code": "^3.5",
        "laminas/laminas-db": "^2.10",
        "laminas/laminas-form": "^3.0",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-math": "^3.2",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-navigation": "^2.9",
        "laminas/laminas-permissions-acl": "^2.7",
        "laminas/laminas-session": "^2.8",
        "laminas/laminas-zendframework-bridge": "^1.3",
        "xenolope/quahog": "^3.0"
    },
    "authors": [
        {
            "name": "Dylan Kerr",
            "email": "dylan-kerr@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "User\\": "module/User/src",
            "Utility\\": "src/Utility"
        }
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8",
        "laminas/laminas-component-installer": "^2.1",
        "laminas/laminas-developer-tools": "^2.0",
        "laminas/laminas-development-mode": "^3.2",
        "rafalli/bjy-profiler": "^1.3",
        "san/san-session-toolbar": "^4.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-install-cmd": [
            "Utility\\ComposerScripts::devModeOnly",
            "cghooks add --ignore-lock"
        ],
        "post-update-cmd": [
            "Utility\\ComposerScripts::devModeOnly",
            "cghooks update"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php"
    },
    "extra": {
        "hooks": {
            "pre-commit": "composer cs-check"
        }
    }
}
