{
    "name": "lochac/seneschals-database",
    "type": "project",
    "require": {
        "google/apiclient": "1.1.4",
        "zendframework/zend-authentication": "^2.7",
        "zendframework/zend-db": "^2.10",
        "zendframework/zend-math": "^3.2",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-navigation": "^2.9",
        "zendframework/zend-permissions-acl": "^2.7",
        "xenolope/quahog": "^2.1"
    },
    "authors": [
        {
            "name": "Dylan Kerr",
            "email": "dylan-kerr@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "User\\": "module/User/src"
        }
    },
    "require-dev": {
        "bjyoungblood/bjy-profiler": "dev-master",
        "san/san-session-toolbar": "^2.0",
        "squizlabs/php_codesniffer": "^3.4",
        "zendframework/zend-component-installer": "^2.1",
        "zendframework/zend-developer-tools": "^1.2",
        "zfcampus/zf-development-mode": "^3.2"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php"
    }
}
